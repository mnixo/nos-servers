{
  "entrypoint": "index.html",
  "shell": "src/nos-servers-app.js",
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/*.js",
    "node_modules/@webcomponents/webcomponentsjs/bundles/*.js"
  ],
  "autoBasePath": true,
  "builds": [
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true
    }
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}
